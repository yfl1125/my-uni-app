/* 水平间距 */
.nav {
  display: inline-block;
  width: 120rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
}
.nav.active {
  color: red;
}
.bt {
  font-size: 20px;
  font-weight: 800;
  margin-top: 30px;
}
.title {
  text-align: center;
  width: 100%;
  height: 120px;
  overflow: auto;
}
._ul {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  padding: 20rpx 40rpx;
}
._ul ._li {
  width: 46%;
  text-align: center;
}
._ul ._li ._img {
  width: 100%;
  height: 240rpx;
}
._ul ._li ._h1 {
  font-size: 30rpx;
  font-weight: 700;
}
._ul ._li ._p {
  color: red;
}

